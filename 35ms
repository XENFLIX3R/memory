local function _(arg1, arg2) -- Line 1205, Named "evalColorSequence"
	if arg2 == 0 then
		return arg1.Keypoints[1].Value
	end
	if arg2 == 1 then
		return arg1.Keypoints[#arg1.Keypoints].Value
	end
	for i_12 = 1, #arg1.Keypoints - 1 do
		local var375 = arg1.Keypoints[i_12]
		local var376 = arg1.Keypoints[i_12 + 1]
		if var375.Time <= arg2 and arg2 < var376.Time then
			local var377 = (arg2 - var375.Time) / (var376.Time - var375.Time)
			return Color3.new((var376.Value.R - var375.Value.R) * var377 + var375.Value.R, (var376.Value.G - var375.Value.G) * var377 + var375.Value.G, (var376.Value.B - var375.Value.B) * var377 + var375.Value.B)
		end
	end
