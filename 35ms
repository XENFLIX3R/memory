					var282 = {}
				end
				tbl_10[categoryName] = var282
				tbl_10[categoryName][skinName_2] = (tbl_10[categoryName][skinName_2] or 0) + 1
			end
		end
	end
	for i_5, v_5 in pairs(tbl_10) do
		if arg1[i_5] then
			for i_6, v_6 in pairs(v_5) do
				if arg1[i_5][i_6] then
					arg1[i_5][i_6] = math.max(0, arg1[i_5][i_6] - v_6)
				end
			end
		end
	end
	return arg1
end
